		prevention,top_corner_config2 = <3 80 600 1124 0>;
		prevention,hor_long_top_corner_config = <1124 3657 0 0>;
		prevention,top_middle_config = <20224 1853 1853 0>;
		prevention,corner_eliminate_point_type = <0 2264 4448 1>;
		prevention,finger_hold_differ_size = <1 0 562 300>;
