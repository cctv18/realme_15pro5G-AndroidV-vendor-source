#include <dt-bindings/msm-camera.h>
#include "ftm_camera_settings.dtsi"
#include "regulator/aw37004.dtsi"

&soc {
	led_flash_rear_wide: qcom,camera-flash1 {
		cell-index = <1>;
		compatible = "qcom,camera-flash";
		flash-source = <&pmxr2230_flash0 &pmxr2230_flash3>;
		torch-source = <&pmxr2230_torch0 &pmxr2230_torch3>;
		switch-source = <&pmxr2230_switch2>;
		status = "ok";
	};

	qcom,cam-res-mgr {
		compatible = "qcom,cam-res-mgr";
		status = "ok";
	};

	oplus,camera_extension@0 {
		compatible = "oplus,camera_extension";
		enable_camera_extension;
		status = "ok";
	};

};

&cam_csiphy3{
	cell-index = <3>;
	compatible = "qcom,csiphy-khFront", "qcom,csiphy";
};

